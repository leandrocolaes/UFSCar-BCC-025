#include <iostream>

using namespace std;

class Ponto2D {
private:
	int x;
	int y;
public:
	Ponto2D() {
		x = 0;
		y = 0;
	}
	Ponto2D(int vx, int vy) {
		x = vx;
		y = vy;
	}
	int get_x(){
		return x;
	}
	int get_y(){
		return y;
	}

};

class Ponto3D : public Ponto2D {
private:
	int z;
public:
	Ponto3D(int vx = 0, int vy = 0, int vz = 0): Ponto2D(vx, vy) {
		z = vz;
	}
	int get_z(){
		return z;
	}
};
	ostream &operator<<(ostream& output, Ponto2D &p1){
		output << "(" << p1.get_x() << ", " << p1.get_y() << ")";
		return output;
	}
	ostream &operator<<(ostream& output, Ponto3D &p1) {
		output << "(" << p1.get_x() << ", " << p1.get_y() << ", " << p1.get_z() << ")";
		return output;
}
int main () {
	Ponto2D p1(1, 1), p2(3,4), p3;
	Ponto3D p4(1, 2, 3);

	cout << p1 << endl;
	cout << p4 << endl;

	return 0;
}
